{
    "title": "MochaUI",
    "styles": {
        "website": "assets/mocha-book-style.css"
    },
    "plugins": [
        "custom-js-css",
        "codeblock-filename",
        "sectionx",
        "prism",
        "-highlight",
        "-sharing",
        "header",
        "copy-code-button",
        "tbfed-pagefooter",
        "include-codeblock",
        "anchor-navigation-ex"
    ],
    "pluginsConfig": {
        "custom-js-css": {
            "js": [
              "assets/mocha-book-script.js"
            ]
        },
        "sectionx": {
            "tag": "b"
        },
        "prism": {
            "css": [
              "prismjs/themes/prism-atom-dark.css"
            ]
        },
        "tbfed-pagefooter": {
            "copyright":"MochaUI Copyright &copy TGideas",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "include-codeblock": {
            "template": "default",
            "unindent": true,
            "theme": "monokai"
        },
        "layout": {
            "headerPath" : "assets/header.html"
        },
        "anchor-navigation-ex": {
            "showLevel": false,
            "associatedWithSummary": true,
            "printLog": false,
            "multipleH1": true,
            "mode": "float",
            "showGoTop":true
        }
    }

}